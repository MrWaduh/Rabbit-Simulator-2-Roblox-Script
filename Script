local OrionLib = loadstring(game:HttpGet(('https://pastebin.com/raw/WRUyYTdY')))()

local Window = OrionLib:MakeWindow({
    Name = "Rabbit Simulator 2 Script",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "OrionTest"
})

-- Main Tab
local MainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

MainTab:AddSection({
    Name = "Created by Mr_Waduh"
})

-- Auto Carrot
MainTab:AddToggle({
    Name = "Auto Carrot",
    Default = false,
    Callback = function(Value)
        Carrot = Value
        while Carrot do
            task.wait()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local backpack = player:WaitForChild("Backpack")
            local toolName = "Carrot"

            local tool = backpack:FindFirstChild(toolName)
            if tool then
                tool.Parent = character
                print(toolName .. " equipped!")
            end

            -- Perform carrot action
            if character:FindFirstChild(toolName) then
                character[toolName].E:FireServer()
            end
        end
    end
})

-- Auto Fight
MainTab:AddToggle({
    Name = "Auto Attack",
    Default = false,
    Callback = function(Value)
        Fight = Value
        while Fight do
            task.wait()
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local backpack = player:WaitForChild("Backpack")
            local toolName = "Fight"

            local tool = backpack:FindFirstChild(toolName)
            if tool then
                tool.Parent = character
                print(toolName .. " equipped!")
            end

            -- Perform fight action
            if character:FindFirstChild(toolName) then
                character[toolName].E:FireServer()
            end
        end
    end
})

-- Auto Rebirth
MainTab:AddToggle({
    Name = "Auto Rebirth",
    Default = false,
    Callback = function(Value)
        Rebirth = Value
        while Rebirth do
            task.wait()
            game:GetService("ReplicatedStorage").RebirthMe:FireServer()
        end
    end
})

-- Auto Reset Size
MainTab:AddToggle({
    Name = "Auto Reset Size",
    Default = false,
    Callback = function(Value)
        Reset = Value
        while Reset do
            task.wait()
            game:GetService("ReplicatedStorage").ResetSize:FireServer()
        end
    end
})

-- Teleport Tab
local TeleportTab = Window:MakeTab({
    Name = "Teleport",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

TeleportTab:AddSection({
    Name = "Created by Mr_Waduh"
})

-- Teleport to Block
TeleportTab:AddButton({
    Name = "Tp to Block",
    Callback = function()
        local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
        local block = workspace:FindFirstChild("Block") and workspace.Block:FindFirstChild("Base")
        if block then
            rootPart.CFrame = block.CFrame
        else
            print("Block not found!")
        end
    end
})

-- Teleport to Sky Carrot
TeleportTab:AddButton({
    Name = "Tp to Sky Carrot",
    Callback = function()
        local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
        local target = workspace:FindFirstChild("Sky Carrot Quest") and workspace["Sky Carrot Quest"]:FindFirstChild("ClaimSkyCarrot")
        if target then
            rootPart.CFrame = target.CFrame
        else
            print("Sky Carrot Quest not found!")
        end
    end
})

-- Teleport to Sky Rabbit
TeleportTab:AddButton({
    Name = "Tp to Sky Rabbit",
    Callback = function()
        local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
        local target = workspace:FindFirstChild("Sky Carrot Quest") and workspace["Sky Carrot Quest"]:FindFirstChild("Sky Rabbit") and workspace["Sky Carrot Quest"]["Sky Rabbit"]:FindFirstChild("Head")
        if target then
            rootPart.CFrame = target.CFrame
        else
            print("Sky Rabbit not found!")
        end
    end
})

-- Teleport to Safe Spot 1
TeleportTab:AddButton({
    Name = "Tp to Safe Spot 1",
    Callback = function()
        local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
        local safeSpot1 = workspace:FindFirstChild("Land") and workspace.Land:FindFirstChild("Trees") and workspace.Land.Trees:FindFirstChild("BigTree") and workspace.Land.Trees.BigTree.Trunk1:FindFirstChild("BaseTrunk")
        if safeSpot1 then
            rootPart.CFrame = safeSpot1.CFrame
        else
            print("Safe Spot 1 not found!")
        end
    end
})

-- Teleport to Safe Spot 2
TeleportTab:AddButton({
    Name = "Tp to Safe Spot 2",
    Callback = function()
        local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
        local safeSpot2 = workspace:GetChildren()[10] and workspace:GetChildren()[10]:FindFirstChild("Trees") and workspace:GetChildren()[10].Trees:GetChildren()[6]:FindFirstChild("Trunk1") and workspace:GetChildren()[10].Trees:GetChildren()[6].Trunk1:FindFirstChild("BaseTrunk")
        if safeSpot2 then
            rootPart.CFrame = safeSpot2.CFrame
        else
            print("Safe Spot 2 not found!")
        end
    end
})

-- Ultra Auto Farm Tab
local AutoFarmTab = Window:MakeTab({
    Name = "Ultra Auto Farm",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

AutoFarmTab:AddToggle({
    Name = "Sigma Boy",
    Default = false,
    Callback = function(Value)
        auto = Value
        while auto do
            task.wait()
            -- Auto Fire Carrot
            game:GetService("Players").LocalPlayer.Character.Carrot.E:FireServer()
            -- Auto Rebirth
            game:GetService("ReplicatedStorage").RebirthMe:FireServer()
            -- Auto Fire Fight
            game:GetService("Players").LocalPlayer.Character.Fight.E:FireServer()

            -- Teleport to Block if it exists
            local rootPart = game.Players.LocalPlayer.Character.HumanoidRootPart
            local block = workspace:FindFirstChild("Block") and workspace.Block:FindFirstChild("Base")
            if block then
                rootPart.CFrame = block.CFrame
            else
                print("No block found yet!")
            end
        end
    end
})

-- Initialize UI
OrionLib:Init()
